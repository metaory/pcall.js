{
  "name": "pcall.js",
  "version": "0.2.1-1",
  "description": "🛟 Protected Asynchronous Operations ▎📦 zero-dependency╶╴🚀 zero-configuration╶╴⚙️ extendable",
  "author": "metaory <metaory@gmail.com>",
  "bugs": "https://github.com/metaory/pcall.js/issues",
  "homepage": "https://github.com/metaory/pcall.js#readme",
  "repository": "git+https://github.com/metaory/pcall.js.git",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.mts",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "npx unbuild",
    "build:stub": "npx unbuild --stub",
    "dev": "node --watch-path=src --watch-path=test/dev.test.js test/dev.test.js"
  },
  "keywords": [
    "promise"
  ],
  "license": "MIT"
}
